		for (int i = 1; i <=30; i ++)
		{
			if(i%7==0) {
				continue;
			}
			else {
				System.out.println("ringing on day" + i);
			}
		}
		
	}

}

class OddEven
{
	
//	this function take one integer as input and checks whether is 
//	even or ad and prints it
	public void checkOddEven(int number)
	{
//		checking whether the number gives remainder as 0 or not
		if(number % 2 == 0)
		{
			System.out.println("the given number is a even number");
		}
		else
		{
			System.out.println("the given number is a odd number number");
		}
	}
	
}
